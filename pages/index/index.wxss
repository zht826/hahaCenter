page{
    width: 100%;
    height: 100%;
    background-color: #f5f5f9;
}
.module{
    float: left;
    width: 330rpx;
    height: 180rpx;
    line-height: 180rpx;
    margin-left: 30rpx;
    margin-top: 20rpx;
    overflow: hidden;
    border: 1px solid #000;
    box-sizing: border-box;
    box-shadow: 0 0 5px #000;
    background-color: #fff;
}
.module navigator{
    width: 100%;
    height: 100%;
}
.module image{
    width: 90rpx;
    height: 90rpx;
    margin-top: 45rpx;
    margin-left: 35rpx;
    margin-right: 35rpx;
    float: left;
}
.module .title{
    width: 140rpx;
    float: left;
    font-size: 30rpx;
}